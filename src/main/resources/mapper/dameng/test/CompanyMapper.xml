<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dameng.master.test.mapper.CompanyMapper">
    <select id="query" resultType="Company">
        select * from t_company
    </select>

    <select id="queryById" resultType="Company">
        select * from t_company where id = #{id}
    </select>
</mapper>